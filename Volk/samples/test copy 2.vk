class Test
{
    int _size;
    int _count;

    constructor (int size, int count)
    {
        this._size = size;
        this._count = count;
    }
}

function void printTest(Test obj)
{
    printf("%d\n", obj._size);
}

Test var = new Test();
var.__ctor(1, 2);
printf("value: \n");
printTest(var);
return 42;